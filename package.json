{
  "name": "carineland",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lighthouse": "lighthouse http://localhost:3000 --view --output html --output-path ./lighthouse-report.html",
    "lighthouse:ci": "lhci autorun",
    "quality": "npm run type-check && npm run lint && npm run format:check && npm run test",
    "process-creations": "node scripts/update-creations.js",
    "optimize-images": "node scripts/optimize-images.js",
    "vercel-build": "next build"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@emotion/cache": "11.14.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "6.5.0",
    "@mui/material": "6.5.0",
    "@mui/material-nextjs": "^7.3.3",
    "csv-parse": "^6.1.0",
    "framer-motion": "11.18.2",
    "next": "^16.0.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@lhci/cli": "^0.15.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/node": "20.19.24",
    "@types/react": "18.3.26",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.0.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lighthouse": "^13.0.1",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "sharp": "^0.34.4",
    "ts-jest": "^29.4.5",
    "typescript": "5.9.3",
    "vercel": "^48.8.0"
  }
}
